<%= render partial: "events/tabs" %>

<div class="row">
  <div class="col-sm-6">
    <h3>Pick your tickets</h3>

    <%= render partial: 'flash' %>

    <%= form_for [@event, @order], url: wizard_path do |f| %>
      <%= fields_for :access_levels do |af| %>
        <table class="table table-hover">
          <%= render partial: "access_level", collection: @access_levels, locals: {af: af} %>
        </table>
      <% end %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
