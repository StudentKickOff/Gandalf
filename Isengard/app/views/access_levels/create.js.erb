$('.form-errors').remove()
<% if @access_level.errors.any? %>
  $('#new_access_level').prepend("<%= escape_javascript form_errors(@access_level) %>")
<% else %>
  if (document.URL.split('=')[1] == 'true') {
    $("<%= escape_javascript render @access_level, advanced: true %>").appendTo("#table-access-levels")
  } else {
    $("<%= escape_javascript render @access_level, advanced: false %>").appendTo("#table-access-levels")
  }

  $("#new_access_level")[0].reset()
<% end %>
